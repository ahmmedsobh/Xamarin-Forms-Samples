<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Xamarin_Forms_Samples.Pages.WorkingWithNavigationPage"
             Title="Working With Navigation Page">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center">
            <Button Text="Push" x:Name="btnPush"/>
            <Button Text="Pop" x:Name="btnPop"/>
            <Button Text="Push Modal" x:Name="btnPushModal"/>
            <Button Text="Pop Modal" x:Name="btnPopModal"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>